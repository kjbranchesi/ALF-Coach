{
  "name": "alf-coach",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:clean": "rimraf playwright-report test-results blob-report || true",
    "audit:showcase": "node --import tsx --loader ./scripts/asset-loader.mjs scripts/audit-showcaseV2.ts",
    "lint:enrich": "node --import tsx --loader ./scripts/asset-loader.mjs scripts/enrichment-lint.ts",
    "generate:enrichment": "node --import tsx --loader ./scripts/asset-loader.mjs scripts/generate-enrichment-tracker.ts",
    "deploy:staging": "npm run build && firebase deploy --only hosting",
    "deploy:production": "npm run build && firebase deploy",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:storage-rules": "firebase deploy --only storage",
    "deploy:all-rules": "firebase deploy --only firestore:rules,storage",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "deploy:netlify": "npm run build && netlify deploy --prod",
    "check:lazy": "node scripts/check-lazy-usage.mjs",
    "generate:hero-schema": "tsx scripts/generate-hero-schema.ts",
    "export:hero-data": "node --import tsx scripts/export-hero-data.ts",
    "lint:curly": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix --rule '{\"curly\": \"error\"}'"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@react-pdf/renderer": "^4.3.0",
    "@rive-app/react-canvas": "^4.22.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jspdf": "^1.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.0",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.3",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.525.0",
    "pako": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-remark": "^2.1.0",
    "react-router-dom": "^7.7.0",
    "react-swipeable": "^7.0.2",
    "react-syntax-highlighter": "^5.8.0",
    "react-use": "^17.6.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tailwind-merge": "^3.3.1",
    "turndown": "^7.2.0",
    "unified": "^11.0.5",
    "uuid": "^11.1.0",
    "zod": "^4.0.5",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.29.0",
    "@iconify/json": "^2.2.377",
    "@iconify/react": "^6.0.0",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/pako": "^2.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.4",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "firebase-tools": "^14.19.1",
    "globals": "^16.2.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.43.1",
    "ts-json-schema-generator": "^2.3.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
